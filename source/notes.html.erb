---
pageable: true
per_page: 10
layout: layout
---

<div class="main-content notes-content">
  <h1>
    <span>Notes</span>
    <span>Inspiration</span>
    <span>Brain dump</span>
  </h1>

  <section class="notes-list">
  <% blog.tags.sort.each do |tag, articles| %>
    <% if (tag == 'note') then %>
      <% articles.sort_by(&:title).each do |article| %>
      <article>
        <img src="<%= article.data.thumbnail_image %>">
        <%= link_to article.title, article %>
      </article>
      <% end %>
    <% end %>
  <% end %>
  </section>
</div>
